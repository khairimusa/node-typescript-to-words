{
  "name": "khairi-number-to-words",
  "version": "1.0.0",
  "description": "The Access Group Assignment - Mohammad Khairi",
  "repository": {
    "type": "git",
    "url": "https://github.com/khairimusa/node-typescript-to-words.git"
  },
  "license": "MIT",
  "author": "Khairi Musa",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "npm run clean && tsc --noEmitOnError",
    "compile": "tsc ./src/Towords.ts",
    "clean": "rimraf dist coverage",
    "commit": "git-cz",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "release": "standard-version",
    "release:mock": "npm run release -- --dry-run",
    "test": "jest --detectOpenHandles --coverage --no-cache",
    "test:watch": "npm run test -- --watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.1.1",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.21",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sort-package-json": "^1.57.0",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chalk-animation": "^2.0.3",
    "figlet": "^1.7.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.14",
    "nanospinner": "^1.1.0"
  }
}
